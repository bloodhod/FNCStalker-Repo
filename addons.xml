<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="repository.FNCStalker-Repo" name="FNC Stalker Repo" version="1.0" provider-name="FreeNetCable">
	<extension point="xbmc.addon.repository" name="FNC Stalker Repository">
		<info compressed="false">http://raw.github.com/bloodhod/FNCStalker-Repo/master/addons.xml</info>
		<checksum>http://raw.github.com/bloodhod/FNCStalker-Repo/master/addons.xml.md5</checksum>
		<datadir zip="true">http://raw.github.com/bloodhod/FNCStalker-Repo/master/zips</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>FNC Stalker Repository</summary>
		<description>Download and install addon. For OFFICIAL Support Visit www.freenetcable.com</description>
		<platform>all</platform>
	</extension>
</addon>

<addon id="script.stalker" name="FNC Stalker" version="2.1.1" provider-name="FreeNetCable">
	<requires>
		<import addon="xbmc.python" version="2.1.0"/>
		<import addon="script.module.simplejson" version="2.0.10"/>
	</requires>
	<extension point="xbmc.python.script" library="run.py"/>
    <extension point="xbmc.service" library="service.py" start="login"/>
	<extension point="xbmc.addon.metadata">
		<language></language>
		<platform>all</platform>
		<license>GPLv2</license>
		<forum></forum>
		<website></website>
		<email></email>
		<source></source>
	</extension>
</addon>
</addons>
